---
import Layout from '../layouts/Layout.astro';
---

<Layout title="SNSDEV">
	<main>
		<h1>SNS <span>DEV</span></h1>
	</main>
</Layout>

<style>
	main {
		display: flex;
		justify-content: center;
		align-items: center;
		height: 100vh;

		padding: 0;
		margin: 0;
	}
	
	h1 {
		margin: 0;

		font-size: 60px;
		font-weight: 700;
		color: #ffffff;
		
		user-select: none;
	}

	h1 span {
		font-weight: 600;
	}
</style>
